            "Add": [
                {
                    "Alias": "@ddghtml",
                    "Description": "Privacy, simplified.",
                    "IconURL": "https://duckduckgo.com/favicon.ico",
                    "Method": "POST",
                    "Name": "DuckDuckGo HTML",
                    "PostData": "q={searchTerms}",
                    "SuggestURLTemplate": "https://html.duckduckgo.com/html/",
                    "URLTemplate": "https://html.duckduckgo.com/html/"
                },
                {
                    "Alias": "@metager",
                    "Description": "Privacy Protected Search & Find",
                    "IconURL": "https://metager.org/img/favicon/favicon-120.png",
                    "Method": "POST",
                    "Name": "MetaGer",
                    "PostData": "eingabe={searchTerms}",
                    "SuggestURLTemplate": "https://metager.org/meta/meta.ger3",
                    "URLTemplate": "https://metager.org/meta/meta.ger3"
                },
                {
                    "Alias": "@searx",
                    "Description": "A privacy-respecting, hackable metasearch engine",
                    "IconURL": "https://searx.be/static/themes/oscar/img/favicon.png",
                    "Method": "POST",
                    "Name": "SearXNG",
                    "PostData": "q={searchTerms}&time_range=&language=en-US&category_general=on",
                    "SuggestURLTemplate": "https://searx.be/",
                    "URLTemplate": "https://searx.be/"
                },
                {
                    "Alias": "@startpage",
                    "Description": "Private Search Engine. No Tracking. No Search History.",
                    "IconURL": "https://www.startpage.com/sp/cdn/favicons/favicon--dark.ico",
                    "Method": "POST",
                    "Name": "StartPage",
                    "PostData": "q={searchTerms}",
                    "SuggestURLTemplate": "https://www.startpage.com/sp/search",
                    "URLTemplate": "https://www.startpage.com/sp/search"
                },
                {
                    "Alias": "@qwant",
                    "Description": "The search engine that doesn't know anything about you, and that changes everything!",
                    "IconURL": "https://www.qwant.com/public/favicon.fc441281cd68c683cd0cc0416f7cb891.ico",
                    "Method": "GET",
                    "Name": "Qwant",
                    "SuggestURLTemplate": "https://www.qwant.com/?q={searchTerms}",
                    "URLTemplate": "https://www.qwant.com/?q={searchTerms}"
                },
                {
                    "Alias": "@qwantlite",
                    "Description": "Qwant Lite",
                    "IconURL": "https://www.qwant.com/public/favicon.fc441281cd68c683cd0cc0416f7cb891.ico",
